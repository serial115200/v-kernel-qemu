{
    // 使用 IntelliSense 了解相关属性。
    // 悬停以查看现有属性的描述。
    // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) 启动 QEMU + GDB",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/linux-6.14.4/vmlinux",
            "miDebuggerServerAddress": "127.0.0.1:1234",
            "preLaunchTask": "启动 QEMU (GDB Server)",
            "postDebugTask": "停止 QEMU",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/linux-6.14.4",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "filterStderr": true,
            "logging": {
                "moduleLoad": false,
                "trace": false,
                "engineLogging": false
            },
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "将反汇编风格设置为 Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }

    ]
}
